{
  "name": "yooogi/dellinsdk",
  "type": "library",
  "description": "PHP SDK для Деловых Линий",
  "keywords": [
	"Деловые Линии",
	"sdk",
	"dellin",
	"dellinsdk",
	"служба доставки",
	"транспортная компания"
  ],
  "require": {
	"php": ">=8.1",
	"ext-intl": "*",
	"ext-json": "*",
	"ext-soap": "*",
	"guzzlehttp/guzzle": "^7.5",
	"psr/log": "^1.1",
	"ext-mbstring": "*"
  },
  "autoload": {
	"psr-4": {
	  "Yooogi\\DellinSDK\\": "src/"
	}
  },
  "license": "MIT",
  "version": "0.1.4",
  "authors": [
	{
	  "name": "Denis Potapov"
	}
  ],
  "require-dev": {
	"vimeo/psalm": "^4.22",
	"phpunit/phpunit": "*"
  },
  "config": {
	"platform-check": false
  },
  "scripts": {
	"psalm": "vendor/bin/psalm --config=psalm.xml"
  }
}
